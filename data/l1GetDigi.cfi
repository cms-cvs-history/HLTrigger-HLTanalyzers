   untracked bool getGctJetCounts    = false
   untracked bool getGctEt           = false
   untracked bool getL1Calo          = false
   untracked bool getGtEvmRR         = false
   untracked bool getGtObjectMap     = false
   untracked bool getGtReadoutRecord = false
   untracked bool getGmtCands        = false
