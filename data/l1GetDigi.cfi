   InputTag GctCenJetCollection = gctDigis:cenJets
   InputTag GctForJetCollection = gctDigis:tauJets
   InputTag GctTauJetCollection = gctDigis:tauJets
   InputTag GctJetCounts = gctDigis

   InputTag GctEtHadCollection = gctDigis
   InputTag GctEtMissCollection = gctDigis
   InputTag GctEtTotalCollection = gctDigis

   InputTag GtEvmReadoutRecord = gtDigis
   InputTag GtObjectMapRecord = gtDigis
   InputTag GtReadoutRecord = gtDigis

   InputTag GmtCands = gmtDigis
